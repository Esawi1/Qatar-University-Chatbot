<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <add name="httpPlatformHandler" path="*" verb="*" modules="httpPlatformHandler" resourceType="Unspecified" />
    </handlers>
    <httpPlatform processPath="D:\home\Python39\python.exe" arguments="D:\home\site\wwwroot\startup.py" stdoutLogEnabled="true" stdoutLogFile="D:\home\LogFiles\stdout.log" startupTimeLimit="60">
      <environmentVariables>
        <environmentVariable name="PYTHONPATH" value="D:\home\site\wwwroot" />
        <environmentVariable name="WEBSITE_SITE_NAME" value="%WEBSITE_SITE_NAME%" />
        <environmentVariable name="WEBSITE_RESOURCE_GROUP" value="%WEBSITE_RESOURCE_GROUP%" />
      </environmentVariables>
    </httpPlatform>
  </system.webServer>
</configuration>
